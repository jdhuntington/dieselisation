<!DOCTYPE html>
<html>
<head>
  <title>Dieselisation</title>
  <%= stylesheet_link_tag    "application" %>
  <%#= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>

  <%= yield :header %>
</head>
<body>

  <%= render :partial => 'layouts/header' %>

  <% flash.each do |k,v| %>
    <div class="flash <%= k %>">
      <%= v %>
    </div>
  <% end %>

  <div class="container">
    <%= yield %>
    <%= render :partial => 'layouts/footer' %>
  </div>
</body>
</html>
